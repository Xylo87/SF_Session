{% extends 'base.html.twig' %}

{% block title %}Listes des stagiaires{% endblock %}

{% block body %}

<div class="wrapper">
    <h1>☣️ Liste des stagiaires ☣️</h1>

    {% for stagiaire in stagiaires %}
        <u>{{stagiaire}}</u><br>
        <ul><li>Email : {{stagiaire.email}}</li><li>Tel : {{stagiaire.tel}}</li></ul><br>
    {% endfor %}
</div>
{% endblock %}
